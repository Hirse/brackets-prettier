{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "brackets": false
  },
  "rules": {
    "prefer-template": "warn",
    "no-var": "error",
    "prettier/prettier": "warn"
  }
}
